{
  "name": "game-squad-backend",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node src/main.server.ts",
    "dev:watch": "ts-node-dev --respawn --transpile-only src/main.server.ts",
    "worker": "ts-node src/main.worker.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.server.js",
    "start:ts": "ts-node src/main.server.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.29.1",
    "stripe": "^14.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
