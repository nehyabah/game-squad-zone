{
  "version": 2,
  "builds": [
    {
      "src": "src/main.server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/main.server.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/main.server.ts": {
      "maxDuration": 30
    }
  }
}